      <div class="jumbotron">
        <h1 class="display-4">Filter</h1>
        <p class="lead">Based on fuzzy logic to search the database</p>
        <hr class="my-4">
        <p>
          <form>
            <div class="form-row">
              <div class="col-md-2 form-group">
                <label for="age">Age</label>
                <input type="text" class="form-control" id="age" placeholder="Number">
              </div>
            <div class="col-md-2 form-group">
              <label for="bloodGroup">Blood Group</label>
              <select class="form-control" id="bloodGroup" placeholder="Select Blood Group">
                <option>A+</option>
                <option>A-</option>
                <option>B+</option>
                <option>B-</option>
                <option>O+</option>
                <option>O-</option>
                <option>AB+</option>
                <option>AB-</option>
              </select>
            </div>

            <div class="col-md-2 form-group">
              <label for="gender">Gender</label>
              <select class="form-control" id="gender">
                <option>Male</option>
                <option>Female</option>
              </select>
            </div>
            <div class="col-md form-group">
              <label for="hospitalName">Hospital Name</label>
              <input type="text" class="form-control" id="hospitalName">
            </div>  

          </div>
            <div class="form-row">
              <div class="col-4">
                <input type="text" class="form-control" placeholder="City">
              </div>
              <div class="col-4">
                <input type="text" class="form-control" placeholder="State">
              </div>
              <div class="col-4">
                <input type="text" class="form-control" placeholder="Country">
              </div>
            </div>
          </form>
        </p>
        <p class="lead">
          <a class="btn btn-primary btn-lg" href="#" role="button" onclick="donorFuzzy()">Donors</a>
          <a class="btn btn-primary btn-lg" href="#" role="button" onclick="patientFuzzy()">Patients</a>
        </p>
      </div>
      <span id="show-filter"></span>
    </div>
  </div>

